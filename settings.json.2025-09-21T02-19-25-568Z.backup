{
  "cleanupPeriodDays": 1000,
  "env": {
    "CLAUDE_CODE": "1"
  },
  "hooks": {
    "PreToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "npx madame-claude-bridge"
          }
        ]
      },
      {
        "matcher": "Task",
        "hooks": [
          {
            "type": "command",
            "command": "/home/jarda/3rd/claude-code-tools/hooks/pretask_subtask_flag.py"
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "braindump --path brain/hooks/test-hook/source",
            "timeout": 10000
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "npx madame-claude-bridge"
          }
        ]
      },
      {
        "matcher": "Task",
        "hooks": [
          {
            "type": "command",
            "command": "/home/jarda/3rd/claude-code-tools/hooks/posttask_subtask_flag.py"
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "npx madame-claude-bridge"
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "/home/jarda/3rd/claude-code-tools/hooks/notification_hook.sh"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "npx madame-claude-bridge"
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "npx madame-claude-bridge"
          }
        ]
      }
    ]
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash"
    ],
    "deny": []
  }
}